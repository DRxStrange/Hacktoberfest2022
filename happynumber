#include<iostream>
using namespace std;
int main()
{long int a[10000],n;
 cin>>n;

 DCM:
 int i=0,l=0;

     while(n>0)

     {
         a[i]=n%10;
       n=n/10;
         i++;

}
    int sum=0;
    for(i=0;i<l;i++)
    {



         sum=a[i]*a[i]+sum;

    }

         if(sum==4||sum==16||sum==37||sum==58||sum==89||sum==145||sum==42||sum==20)
         {
             cout<<"false";

}

else if(sum==1)
{      cout<<"true";
}
          else if(sum!=1)
     {    n=sum;
         goto DCM;
}

    }
